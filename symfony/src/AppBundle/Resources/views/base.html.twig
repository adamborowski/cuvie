<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}{{ 'tabTitle'|trans }}{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets
        "@AppBundle/Resources/public/bootstrap.min.css" %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
        {% stylesheets filter="compass" output="styles.css"
        "@AppBundle/Resources/assets/css/main.scss" %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% include "AppBundle::header.html.twig" %}
<div class="a-page-content">
    {% block body %}{% endblock %}
</div>


{% block javascripts %}
    {% javascripts
    'https://code.jquery.com/jquery-1.10.2.min.js'
    '@AppBundle/Resources/public/bootstrap.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript">
        $(function () {
            $('.container').addClass('a-visible');
        });
    </script>
{% endblock %}
</body>
</html>
